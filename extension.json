{
	"name": "BlockASN",
	"version": "1.1.0",
	"author": [
		"Ryan Schmidt"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:BlockASN",
	"descriptionmsg": "blockasn-desc",
	"license-name": "MIT",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.27.0"
	},
	"config": {
		"BAApiField": {
			"asn": "traits.autonomous_system_number",
			"proxy": [
				"traits.is_anonymous_vpn",
				"traits.is_hosting_provider",
				"traits.is_public_proxy",
				"traits.is_tor_exit_node"
			],
			"type": "traits.user_type"
		},
		"BlockedASNs": [],
		"BlockedUserTypes": []
	},
	"MessagesDirs": {
		"BlockASN": [
			"i18n"
		]
	},
	"Hooks": {
		"GetBlockedStatus": [
			"BlockASN::onGetBlockedStatus"
		]
	},
	"AutoloadClasses": {
		"BlockASN": "BlockASN.php"
	},
	"manifest_version": 1
}
